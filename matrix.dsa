#include <stdio.h>
int main() {
int matrix[4][4];
    
printf("Enter the elements of the matrix:\n");
for (int i = 0; i < 4; i++) {
for (int j = 0; j < 4; j++) {
scanf("%d", &matrix[i][j]);
}
  }
printf("The matrix is:\n");
for (int i = 0; i < 4; i++) {
for (int j = 0; j < 4; j++) {
printf("%d ", matrix[i][j]);
        }
printf("\n");
    }
    
    return 0;
}
